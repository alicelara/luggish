<div class="nested-fields">
	<div class="field" style="display: inline-block;">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>
	<div class="field"  style="display: inline-block;">
		<%= f.label :quantity, "Qty" %>
		<%= f.number_field :quantity %>
	</div>
	<div class="field" style="display: inline-block;">
		<%= f.label :note %>
		<%= f.text_field :note %>
	</div>
</div>
